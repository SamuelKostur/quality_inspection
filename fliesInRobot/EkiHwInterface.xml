<ETHERNETKRL>
	<CONFIGURATION>
		<EXTERNAL>
			<IP>192.168.1.2</IP>
            <PORT>54603</PORT>
		</EXTERNAL>
		<INTERNAL>
			<PROTOCOL>TCP</PROTOCOL>
			<ALIVE Set_Flag="1" />
		</INTERNAL>
	</CONFIGURATION>

	<RECEIVE>
      <XML>
         <!-- Joint position command -->
         <ELEMENT Tag="RobotCommand/Position/@X" Type="REAL" />
         <ELEMENT Tag="RobotCommand/Position/@Y" Type="REAL" />
         <ELEMENT Tag="RobotCommand/Position/@Z" Type="REAL" />
         <ELEMENT Tag="RobotCommand/Orientation/@A" Type="REAL" />
         <ELEMENT Tag="RobotCommand/Orientation/@B" Type="REAL" />
         <ELEMENT Tag="RobotCommand/Orientation/@C" Type="REAL" Set_Flag="2"/>
      </XML>
   </RECEIVE>

   <SEND>
      <XML>
         <!-- $POS_ACT_MES, Measured cartesian robot pose (Robot TCP in relation to the base coord sys) -->
         <ELEMENT Tag="RobotState/Position/@X"/>
         <ELEMENT Tag="RobotState/Position/@Y"/>
         <ELEMENT Tag="RobotState/Position/@Z"/>
         <ELEMENT Tag="RobotState/Orientation/@A"/>
         <ELEMENT Tag="RobotState/Orientation/@B"/>
         <ELEMENT Tag="RobotState/Orientation/@C"/>
      </XML>
   </SEND>
</ETHERNETKRL>
